        <contextMenu idMso="ContextMenuCell">
			<button idMso="PasteValues" insertAfterMso="Copy" imageMso="ContactCardPaste" label="Вставить значения" enabled="true" />
        </contextMenu>
    </contextMenus-->
</customUI>
